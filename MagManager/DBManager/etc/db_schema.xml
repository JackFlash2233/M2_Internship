<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="new_table" resource="default" engine="innodb" comment="for creating Table">
        <column xsi:type="int" name="entity_id" unsigned="true" nullable="false" identity="true"
                comment="Entity ID"/>
        <column xsi:type="varchar" name="column_name" nullable="false" length="64" comment="column_name"/>
        <column xsi:type="varchar" name="column_type" nullable="false" length="64" comment="column_type"/>
        <column xsi:type="varchar" name="null" nullable="false" length="1024" comment="null or not null"/>

        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
    </table>
    <table name="table_column" resource="default" engine="innodb" comment="for column">
        <column xsi:type="varchar" name="COLUMN_NAME" nullable="false" length="64" comment="column_name"/>
        <column xsi:type="varchar" name="data_type" nullable="false" length="64" comment="DATA_TYPE"/>
        <column xsi:type="varchar" name="nullable" nullable="false" length="64" comment="NULLABLE"/>
        <column xsi:type="varchar" name="primary" nullable="false" comment="PRIMARY"/>
    </table>


